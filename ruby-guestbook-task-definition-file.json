{
  "family": "guestbook-taskdefinitions",
  "containerDefinitions": [
    {
      "name": "guestbook",
      "image": "672721203026.dkr.ecr.eu-central-1.amazonaws.com/provision/ruby-docker-build:${VERSION}",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 9292,
          "hostPort": 8000
        }
      ],
      "memory": 256,
      "cpu": 100,
      "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "guestbook-prod-eu-central-1",
                    "awslogs-region": "eu-central-1",
                    "awslogs-stream-prefix": "guestbook-logs"
                }
            },
    }
  ]
}
